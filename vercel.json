{
  "version": 2,
  "name": "hfdx-dex",
  "public": true,
  "rewrites": [
    {
      "source": "/:lng/futures/:symbol",
      "destination": "/index.html"
    },
    {
      "source": "/:lng/futures",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sdk/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/custom.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}

